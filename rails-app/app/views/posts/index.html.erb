<p id="notice"><%= notice %></p>

<h1>投稿一覧</h1>

<table>
  <thead>
  <tr>
    <%= render 'thead' %>
  </tr>
  </thead>
  <tbody>
  <% @posts.each do |post| %>
    <tr>
      <%= render 'content', post: post %>
      <td><%= link_to '見る', post %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>


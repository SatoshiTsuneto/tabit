<h1>自分の投稿</h1>

<table>
  <thead>
  <%= render 'thead' %>
  </thead>
  <tbody>
  <% @posts.each do |post| %>
    <% if post.username == current_user.username %>
      <tr>
        <%= render 'content', post: post %>
        <td><%= link_to '見る', post %></td>
      </tr>
    <% end %>
  <% end %>
  </tbody>
</table>